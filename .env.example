# Variables de Entorno - ARCSAPP

# Supabase
NEXT_PUBLIC_SUPABASE_URL=your-project-url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# Supabase Service Role (obtener del dashboard de Supabase > Settings > API)
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# OpenAI (para análisis IA de documentos)
OPENAI_API_KEY=your-openai-api-key-here

# Google Gemini (para análisis IA de documentos)
GOOGLE_AI_API_KEY=your-google-ai-api-key-here

# Resend (para notificaciones por email)
RESEND_API_KEY=your-resend-api-key-here

# Configuración
MAX_FILE_SIZE_MB=2
